{
  "name": "@dynui-max/playground",
  "version": "0.2.1",
  "private": true,
  "description": "Interactive playground for testing DynUI-Max components",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "pnpm --filter @dynui-max/design-tokens build && pnpm --filter @dynui-max/core build",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli dist"
  },
  "dependencies": {
    "@dynui-max/core": "workspace:*",
    "@dynui-max/design-tokens": "workspace:*",
    "react": "catalog:core",
    "react-dom": "catalog:core"
  },
  "devDependencies": {
    "@types/react": "catalog:core",
    "@types/react-dom": "catalog:core",
    "@vitejs/plugin-react": "catalog:build",
    "typescript": "catalog:typescript",
    "vite": "catalog:build",
    "del-cli": "catalog:tooling"
  }
}
